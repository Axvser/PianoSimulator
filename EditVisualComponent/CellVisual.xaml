<local:VisualUnitControlBase x:Class="PianoSimulator.EditVisualComponent.CellVisual"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PianoSimulator.EditVisualComponent"
             xmlns:mn="clr-namespace:MinimalisticWPF;assembly=MinimalisticWPF"
             mc:Ignorable="d" 
             x:Name="CellBox"
             Height="60"
             Width="12.5">
    <Grid Background="Transparent">
        <Grid.RowDefinitions>
            <RowDefinition Height="5"/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <mn:MButton Text="" WiseHeight="5" WiseWidth="12.5" Grid.Row="0"/>
        <mn:MButton Text="" WiseHeight="13.75" WiseWidth="12.5" Grid.Row="1"/>
        <mn:MButton Text="" WiseHeight="13.75" WiseWidth="12.5" Grid.Row="2"/>
        <mn:MButton Text="" WiseHeight="13.75" WiseWidth="12.5" Grid.Row="3"/>
        <mn:MButton Text="" WiseHeight="13.75" WiseWidth="12.5" Grid.Row="4"/>
    </Grid>
</local:VisualUnitControlBase>
